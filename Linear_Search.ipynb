   "source": [
    "def linear_search(key, l):\n",
    "    index = 0\n",
    "    while(len(l)):\n",
    "        if l[index] == key:\n",
    "            return index\n",
    "        index = index + 1\n",
    "    return -1\n",
    "\n",
    "key = 3\n",
    "l = [2,3,5,5,1,6,6,7,1]\n",
    "print(linear_search(key,l))"
   ]
